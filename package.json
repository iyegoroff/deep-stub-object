{
  "name": "deep-stub-object",
  "version": "2.0.2",
  "description": "Proxy-backed custom error message instead of `TypeError: x is undefined`",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "react-native": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint -- --fix && npm run build && npm t",
    "postversion": "git add . && git commit -am lint || : && git push && git push --tags",
    "build:dts": "tsc -p configs/tsconfig.dts.json",
    "build:esm": "tsc -p configs/tsconfig.esm.json",
    "build:cjs": "build-for-cjs configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs",
    "lint": "eslint .",
    "test": "jest",
    "coverage": "codecov"
  },
  "keywords": [
    "mocking",
    "error-handling"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/deep-stub-object/issues"
  },
  "homepage": "https://github.com/iyegoroff/deep-stub-object#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/deep-stub-object.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "build-for-cjs": "^0.0.8",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
